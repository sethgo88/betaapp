generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Route {
  id    Int    @id @default(autoincrement())
  name  String
  type  String
  grade String
  moves Move[]
}

model Move {
  id      Int    @id @default(autoincrement())
  type    String
  text    String
  Route   Route? @relation(fields: [routeId], references: [id])
  routeId Int?
}
